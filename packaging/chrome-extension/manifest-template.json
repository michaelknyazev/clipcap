{
  "name": "ClipCap - AI Generated Summaries",
  "description": "Chrome Extension",
  "version": "{%version%}",
  "manifest_version": 3,
  "homepage_url": "https://clipcap.app",
  "content_scripts": [
    {
      "all_frames": false,
      "js": [{%js%}],
      "css": [{%css%}],
      "matches": [
        "https://youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": {%bg_js%}
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage", 
    "identity"
  ],
  "host_permissions": [
    "https://youtube.com/*",
    "https://www.youtube.com/*",
    "http://localhost:8080/*"
  ],
  "key": ""
}