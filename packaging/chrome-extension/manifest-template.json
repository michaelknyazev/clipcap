{
  "name": "ClipCap - AI Generated Summaries",
  "description": "ClipCap - Your GPT-Powered Web Content Summarizer.",
  "version": "{%version%}",
  "manifest_version": 3,
  "homepage_url": "https://clipcap.app",
  "content_scripts": [
    {
      "all_frames": false,
      "js": [{%js%}],
      "css": [{%css%}],
      "matches": [
        "https://youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": {%bg_js%}
  },
  "action": {
    "default_popup": "index.html"
  },
  "key": ""
}